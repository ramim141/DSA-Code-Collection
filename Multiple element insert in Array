#include <stdio.h>
#define MAX_SIZE 100

int main()
{
    int A[MAX_SIZE];
    int i, size, num;

    printf("Enter the size of Array: ");
    scanf("%d", &size);

    printf("Enter the element of Array; ");
    for (int i = 0; i < size; i++)
    {
        scanf("%d", &A[i]);
    }

    printf("\nEnter the number of element to insert: ");
    scanf("%d", &num);

    for (int i = size; i < size + num; i++)
    {
        scanf("%d", &A[i]);
    }
    size = size + num;

    printf("Array after insertion: \n");
    for (int i = 0; i < size; i++)
    {
        printf("%d ", A[i]);
    }
    return 0;
}
5
